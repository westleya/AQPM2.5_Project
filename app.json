{
  "expo": {
    "name": "Personal Air Quality Exposure",
    "description": "Shows a report of your PM2.5 exposure",
    "icon":"./assets/images/title.png",
    "slug": "AQ&UProject",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier":"com.universityofutah.airqualitytracker"
    },
    "android":{
      "package":"com.universityofutah.airqualitytracker"
    }
  }
}
